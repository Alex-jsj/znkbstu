{
    "presets": [
        ["env", {
            "modules": false,
            "targets": {
                "browsers": ["> 1%",
                    "not ie <= 8",
                    "iOS >= 7",
                    "Android > 4.1",
                    "Firefox > 20",
                    "last 2 versions"
                ]
            }
        }],
        "stage-2"
    ],
    "plugins": ["transform-vue-jsx", "transform-runtime"]
}
